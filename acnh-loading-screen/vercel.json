{
  "builds": [
    {
      "src": "api/json-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/json-server.js"
    }
  ]
}